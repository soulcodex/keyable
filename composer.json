{
    "name": "soulcodex/keyable",
    "description": "Allow attach api key to laravel models",
    "keywords": [
        "soulcodex",
        "keyable"
    ],
    "homepage": "https://github.com/soulcodex/keyable",
    "license": "MIT",
    "authors": [
        {
            "name": "Roberto Garcia",
            "email": "info@soulcodex.es",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.1",
        "illuminate/contracts": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Soulcodex\\Keyable\\": "src",
            "Soulcodex\\Keyable\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soulcodex\\Keyable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Soulcodex\\Keyable\\KeyableServiceProvider"
            ],
            "aliases": {
                "Skeleton": "Soulcodex\\Keyable\\Facades\\Keyable"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
